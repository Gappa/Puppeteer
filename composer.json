{
	"name": "nelson/puppeteer",
	"require": {
		"php": ">=8.0 <8.2",
		"symfony/process": "^4.3",
		"nette/utils": "^3.0",
		"nette/http": "^3.0",
		"nette/di": "^3.0"
	},
	"require-dev": {
		"nette/tester": "^2.0",
		"phpstan/extension-installer": "^1.0",
		"phpstan/phpstan": "^1.00",
		"phpstan/phpstan-nette": "^1.0.0"
	},
	"autoload": {
		"psr-4": {
			"Nelson\\Puppeteer\\": "src/"
		}
	},
	"scripts": {
		"phpstan": "@php ./vendor/bin/phpstan analyse --configuration ./phpstan/phpstan.neon --memory-limit 512M"
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
