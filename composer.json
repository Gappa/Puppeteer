{
	"name": "nelson/puppeteer",
	"require": {
		"php": "~8.2.0 || ~8.3.0",
		"symfony/process": "^7.0",
		"nette/utils": "^3.2 || ^4.0",
		"nette/http": "^3.1",
		"nette/di": "^3.0"
	},
	"require-dev": {
		"nette/tester": "^2.0",
		"phpstan/extension-installer": "^1.0",
		"phpstan/phpstan": "^1.00",
		"phpstan/phpstan-nette": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.5"
	},
	"autoload": {
		"psr-4": {
			"Nelson\\Puppeteer\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
